{
	"$schema": "https://raw.githubusercontent.com/martinring/tmlanguage/master/tmlanguage.json",
	"scopeName": "source.shell.serenity",
	"name":"Shell (SerenityOS)",
	"patterns": [
		{
			"include": "#comment"
		},
		{
			"include": "#pipeline"
		},
		{
			"include": "#string"
		},
		{
			"include": "#function"
		},
		{
			"include": "#variable"
		},
		{
			"include": "#heredoc"
		},
		{
			"include": "#redirection"
		},
		{
			"include": "#keyword"
		}
	],
	"repository": {
		"comment": {
			"name":"comment"
		},
		"pipeline":{
			"name":"pipeline"
		},
		"string":{
			"name":"string"
		},
		"function": {
			"name": "function.shell.serenity",
			"begin": "",
			"beginCaptures": {},
			"end": "",
			"endCaptures": {},
			"patterns": []
		},
		"variable":{
			"name": "variable.shell.serenity"
		},
		"heredoc":{
			"name":"heredoc.shell.serenity",
			"begin": "",
			"end": ""
		},
		"redirection":{
			"name":"redirection.shell.serenity",
			"begin": "",
			"end": "",
			"patterns": []
		},
		"keyword":{
			"name": "keyword.shell.serenity",
			"match": "[a-zA-Z\\-_]"
		},
		"argument-list": {
			"name": "meta.parameter.shell.serenity",
			"begin": "(\\()",
			"beginCaptures": {},
			"end": "(\\))",
			"endCaptures": {},
			"patterns": []
		}
	}
}